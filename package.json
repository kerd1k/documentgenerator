        "json",
        "jsdoc",
        "javadoc",
        "comments",
        "document",
        "generator"
    ],
    "author": {
        "name": "Aleksejs Cetverikovs",
        "email": "kerd1k@gmail.com"
    },
    "license": "ISC",
    "description": "Generate Json from JavaDoc comments",
    "bin": {
        "documentgenerator": "./dist/index.js"
    },
    "dependencies": {
        "comment-parser": "^1.4.1"
    },
    "devDependencies": {
        "@types/node": "^22.8.2",
        "tsup": "^8.3.5",
        "typescript": "^5.6.3"
